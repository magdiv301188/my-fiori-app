{
  "_version": "1.76.0",
  "sap.app": {
    "id": "resource",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.6",
      "toolsId": "fbb92fd6-4e2a-4e3e-bf95-9dcec9554288"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.144.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.ui.comp": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "resource.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "resource.view",
        "async": true
      },
      "routes": [
    {
      "name": "list",
      "pattern": "",
      "target": "list"
    },
     {
    "name": "create",
    "pattern": "create",
    "target": "detail"
  }
  ],

  "targets": {
    "list": {
      "viewName": "List",
      "viewId": "list"
    },
    "detail": {
      "viewName": "Detail",
      "viewId": "detail"      
  } 
}
    },
    "rootView": {
      "viewName": "resource.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
